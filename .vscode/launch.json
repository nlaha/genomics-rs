  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'genomics-rs'",
      "cargo": {
        "args": ["build", "--bin=genomics-rs", "--package=genomics-rs"],
        "filter": {
          "name": "genomics-rs",
          "kind": "bin"
        }
      },
      "env": {
        "RUST_LOG": "trace"
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'genomics-rs'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=genomics-rs",
          "--package=genomics-rs"
        ],
        "filter": {
          "name": "genomics-rs",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
